---
import MainLayout from "../layout/MainLayout.astro";
---
<MainLayout
  title="Alexander Project"
  seoTitle="Projects with React, Astro"
  seoDesc="Award winning projects/app with Astro, React and NextJS"
>
  <div class="w-8/12 lg:w-6/12 mx-auto">
    <h1 class="text-5xl my-2">Projects</h1>
    <div
      class="grid md:grid-cols-2 gap-2 drop-shadow-[0px_35px_35px_rgba(39,245,131,0.6)]"
    >
      <a href="/project/JumpBox"
        ><img
          class="rounded"
          src="/public/images/JumpBox.png"
          width="300px"
          height="300px"
        /></a
      >
      <a href="/project/FloraQuesst"
        ><img
          class="rounded"
          src="/public/images/FloraQuesst.png"
          width="300px"
          height="300px"
        /></a
      >
      <a href="/project/EscapeFromAlex"
        ><img
          class="rounded"
          src="/public/images/EscapeFromAlex.png"
          width="300px"
          height="300px"
        /></a
      >
      <a href="/project/AlexPortfolio"
        ><img
          class="rounded"
          src="/public/images/IconAlex.jpg"
          width="300px"
          height="300px"
        /></a
      >
    </div>
  </div>
</MainLayout>

